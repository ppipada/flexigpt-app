{
	"name": "frontend",
	"version": "1.0.0",
	"description": "Frontend for FlexiGPT",
	"type": "module",
	"private": true,
	"scripts": {
		"lint:knip": "knip --tags=-lintignore",
		"lint:eslint": "eslint --env-info && eslint . -c ./eslint.config.mjs",
		"lint": "pnpm run typecheck && pnpm run lint:eslint && pnpm run lint:knip",
		"lint:fix": "eslint . -c ./eslint.config.mjs --fix .",
		"fmt": "prettier -c . --ignore-path ../.prettierignore",
		"fmt:fix": "prettier -w . --ignore-path ../.prettierignore",
		"clean": "rimraf ./dist",
		"run:wailsdev": "cp .env.dev .env && cross-env VITE_PLATFORM=wails react-router dev",
		"build:wails": "rimraf ./dist && cp .env.prod .env && pnpm run typecheck && cross-env VITE_PLATFORM=wails react-router build",
		"typecheck": "react-router typegen && tsc",
		"test": "vitest run",
		"coverage": "vitest run --coverage"
	},
	"keywords": [],
	"author": "ppipada",
	"dependencies": {
		"@fontsource-variable/inter": "^5.2.6",
		"@react-router/dev": "^7.7.0",
		"@react-router/node": "^7.7.0",
		"compromise": "^14.14.4",
		"isbot": "^5.1.28",
		"katex": "^0.16.22",
		"mermaid": "^11.8.0",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"react-icons": "^5.3.0",
		"react-markdown": "^10.1.0",
		"react-router": "^7.7.0",
		"react-textarea-autosize": "^8.5.9",
		"rehype-katex": "^7.0.1",
		"rehype-raw": "^7.0.0",
		"remark-gemoji": "^8.0.0",
		"remark-gfm": "^4.0.0",
		"remark-math": "^6.0.0",
		"remark-supersub": "^1.0.0",
		"shiki": "^3.7.0",
		"sprintf-js": "^1.1.3",
		"stopword": "^3.1.5",
		"uuid": "^11.0.3"
	},
	"devDependencies": {
		"@eslint/js": "^9.32.0",
		"@tailwindcss/vite": "^4.1.11",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/react": "^16.3.0",
		"@testing-library/user-event": "^14.6.1",
		"@trivago/prettier-plugin-sort-imports": "^5.2.2",
		"@types/node": "^20.17.6",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"@types/sprintf-js": "^1.1.4",
		"@types/stopword": "^2.0.3",
		"@types/uuid": "^10.0.0",
		"@vitest/coverage-v8": "^3.2.4",
		"cross-env": "^7.0.3",
		"daisyui": "^5.0.46",
		"eslint": "^9.32.0",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.3",
		"eslint-plugin-react": "^7.37.5",
		"globals": "^16.3.0",
		"jiti": "^2.4.2",
		"jsdom": "^26.1.0",
		"knip": "^5.62.0",
		"prettier": "^3.6.2",
		"react-router-devtools": "^5.0.6",
		"rimraf": "^6.0.1",
		"tailwindcss": "^4.1.11",
		"typescript": "^5.8.3",
		"typescript-eslint": "^8.38.0",
		"vite": "^6.3.5",
		"vite-plugin-checker": "^0.10.0",
		"vite-tsconfig-paths": "^5.1.3",
		"vitest": "^3.2.4"
	},
	"knip": {
		"ignore": [
			"app/apis/wailsjs/runtime/runtime.js",
			"app/apis/wailsjs/go/main/App.js",
			"app/apis/wailsjs/go/main/ConversationCollectionWrapper.js",
			"app/apis/wailsjs/go/main/ProviderSetWrapper.js",
			"app/apis/wailsjs/go/main/SettingStoreWrapper.js",
			"app/apis/wailsjs/go/main/ModelPresetStoreWrapper.js",
			"app/apis/wailsjs/go/main/PromptTemplateStoreWrapper.js",
			"app/spec/aiprovider.ts",
			"app/hooks/highlight_worker.ts"
		],
		"ignoreBinaries": [],
		"ignoreDependencies": [
			"@react-router/node",
			"isbot",
			"daisyui",
			"react-router-devtools",
			"tailwindcss",
			"@fontsource-variable/inter",
			"shiki",
			"@testing-library/react",
			"@testing-library/user-event"
		]
	},
	"prettier": {
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"jsxSingleQuote": false,
		"printWidth": 120,
		"proseWrap": "preserve",
		"quoteProps": "as-needed",
		"requirePragma": false,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5",
		"plugins": [
			"@trivago/prettier-plugin-sort-imports"
		],
		"importOrder": [
			"^react$",
			"<THIRD_PARTY_MODULES>",
			"^@/spec/(.*)$",
			"^@/lib/(.*)$",
			"^@/hooks/(.*)$",
			"^@/apis/(.*)$",
			"^@/components/(.*)$",
			"^@/(.*)$",
			"^[./]"
		],
		"importOrderSeparation": true,
		"importOrderSortSpecifiers": true,
		"importOrderCaseInsensitive": true
	}
}
